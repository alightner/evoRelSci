  month = mar,
  year = {2018},
  keywords = {Behavior,Buddhism,Children,Christianity,Cross-cultural studies,Evolutionary theory,Hinduism,Religion},
  pages = {e0193856},
  file = {/Users/aaronlightner/Zotero/storage/APN36C4Z/Purzycki et al. - 2018 - Material security, life history, and moralistic re.pdf;/Users/aaronlightner/Zotero/storage/7BPK86HP/article.html}
